{
  "name": "registre-extractor",
  "version": "1.0.0",
  "description": "Document extraction service for Quebec Land Registry",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "playwright install chromium",
    "dev": "tsx watch src/worker/index.ts",
    "monitor:dev": "tsx watch src/monitor/index.ts",
    "ocr:dev": "tsx watch src/ocr/monitor.ts",
    "build": "tsc && cp src/api/dashboard.html dist/api/",
    "start": "node dist/worker/index.js",
    "monitor": "node dist/monitor/index.js",
    "ocr": "node dist/ocr/monitor.js",
    "api:dev": "tsx watch src/api/index.ts",
    "api:start": "node dist/api/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "docker:generate": "node scripts/generate-docker-compose.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "diagnose": "tsx diagnose-system.ts",
    "test:acte:validate": "tsx validate-acte-ocr-setup.ts",
    "test:acte:standalone": "tsx test-acte-ocr.ts",
    "test:acte:integration": "tsx test-acte-ocr-integration.ts",
    "test:acte:all": "./run-all-acte-tests.sh",
    "diagnose:ocr": "tsx diagnose-ocr-row.ts",
    "migrate:boosted": "tsx apply-boosted-content-migration.ts",
    "check-redis": "tsx check-redis.ts",
    "reset-stuck-jobs": "tsx reset-stuck-jobs.ts",
    "reset-stuck-ocr-jobs": "tsx reset-stuck-ocr-jobs.ts",
    "test:ocr": "tsx test-ocr-integration.ts",
    "test:ocr-recovery": "tsx test-ocr-recovery.ts",
    "test:claude-fallback": "tsx test-claude-fallback.ts"
  },
  "keywords": [
    "web-scraping",
    "agentql",
    "quebec",
    "land-registry"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.39.3",
    "agentql": "^1.13.0",
    "bull": "^4.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.40.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
