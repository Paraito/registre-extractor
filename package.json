{
  "name": "registre-extractor",
  "version": "1.0.0",
  "description": "Document extraction service for Quebec Land Registry",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "playwright install chromium",
    "dev": "tsx watch src/worker/index.ts",
    "build": "tsc && cp src/api/dashboard.html dist/api/ && cp -r src/ocr/prompts dist/ocr/",
    "start": "node dist/worker/index.js",
    "monitor:dev": "tsx watch src/monitor/index.ts",
    "monitor": "node dist/monitor/index.js",
    "api:dev": "tsx watch src/api/index.ts",
    "api:start": "node dist/api/index.js",
    "ocr:dev": "tsx watch src/ocr/start-ocr-workers.ts",
    "ocr:start": "node dist/ocr/start-ocr-workers.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "web-scraping",
    "agentql",
    "quebec",
    "land-registry"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.39.3",
    "agentql": "^1.13.0",
    "bull": "^4.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.40.1",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
