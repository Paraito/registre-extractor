{
  "name": "registre-extractor",
  "version": "1.0.0",
  "description": "Document extraction service for Quebec Land Registry",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/worker/index.ts",
    "dev:v2": "tsx watch src/worker/worker-v2.ts",
    "municipal:dev": "tsx watch src/municipal-extractor-v2/worker/ai-worker.ts",
    "municipal:api:dev": "tsx watch src/municipal-extractor-v2/api/municipal-api.ts",
    "build": "tsc && cp src/api/dashboard.html dist/api/",
    "build:municipal": "tsc && mkdir -p dist/municipal-extractor-v2",
    "start": "node dist/worker/index.js",
    "start:v2": "node dist/worker/worker-v2.js",
    "municipal:start": "node dist/municipal-extractor-v2/worker/ai-worker.js",
    "municipal:api:start": "node dist/municipal-extractor-v2/api/municipal-api.js",
    "api:dev": "tsx watch src/api/index.ts",
    "api:dev:v2": "tsx watch src/api/api-v2.ts",
    "api:start": "node dist/api/index.js",
    "api:start:v2": "node dist/api/api-v2.js",
    "test": "jest",
    "test:municipal": "jest src/municipal-extractor-v2/**/*.test.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "typecheck:municipal": "tsc --noEmit src/municipal-extractor-v2/**/*.ts",
    "docker:generate": "node scripts/generate-docker-compose.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["web-scraping", "agentql", "quebec", "land-registry"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "agentql": "^1.13.0",
    "@supabase/supabase-js": "^2.39.3",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "bull": "^4.12.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "playwright": "^1.40.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/bull": "^4.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "js-yaml": "^4.1.0"
  }
}