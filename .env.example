# Production Supabase Configuration
PROD_SUPABASE_URL=your-prod-supabase-url
PROD_SUPABASE_ANON_KEY=your-prod-anon-key
PROD_SUPABASE_SERVICE_KEY=your-prod-service-key

# Staging Supabase Configuration (optional)
STAGING_SUPABASE_URL=
STAGING_SUPABASE_ANON_KEY=
STAGING_SUPABASE_SERVICE_KEY=

# Development Supabase Configuration (optional)
DEV_SUPABASE_URL=
DEV_SUPABASE_ANON_KEY=
DEV_SUPABASE_SERVICE_KEY=

# API Configuration
API_PORT=3000
API_HOST=0.0.0.0

# Redis Configuration (for Bull queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Worker Configuration
WORKER_CONCURRENCY=20
EXTRACTION_TIMEOUT=180000
SESSION_TIMEOUT=240000

# Logging
LOG_LEVEL=info

# Quebec Registry Configuration
REGISTRY_BASE_URL=https://www.registrefoncier.gouv.qc.ca/Sirf/

# Environment
NODE_ENV=development

# AgentQL Configuration
AGENTQL_API_KEY=your-agentql-api-key
USE_AI_EXTRACTOR=true

# OpenAI Configuration (for vision fallback)
OPENAI_API_KEY=your-openai-api-key

# Gemini AI Configuration (for OCR processing)
GEMINI_API_KEY=your-gemini-api-key

# OCR Configuration
OCR_EXTRACT_MODEL=gemini-2.0-flash-exp
OCR_BOOST_MODEL=gemini-2.5-pro
OCR_EXTRACT_TEMPERATURE=0.1
OCR_BOOST_TEMPERATURE=0.2
OCR_POLL_INTERVAL_MS=10000
OCR_TEMP_DIR=/tmp/ocr-processing